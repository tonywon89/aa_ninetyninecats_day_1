<h1><%= @cat.name %></h1>


<table  border="1">
  <tr>
    <th> Start Date </th>
    <th> End Date </th>
    <th> Status </th>
  </tr>

  <% @cat_rentals.each do |rental| %>
    <tr>
      <td><%= rental.start_date %></td>
      <td><%= rental.end_date %></td>
      <td><%= rental.status %></td>
      <td>
        <%= link_to "Update Request", cat_rental_requests_url(rental) %>
      </td>
    </tr>
    <% end %>
</table>
