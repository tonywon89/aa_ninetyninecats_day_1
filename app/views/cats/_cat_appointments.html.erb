
<table  border="1">
  <tr>
    <th> Start Date </th>
    <th> End Date </th>
    <th> Status </th>
  </tr>

  <% @cat_rentals.each do |rental| %>
    <tr>
      <td><%= rental.start_date %></td>
      <td><%= rental.end_date %></td>
      <td><%= rental.status %></td>
      <td>
        <%= link_to "Update Request", edit_cat_rental_request_url(rental) %>
      </td>
      <td>
        <%= button_to "Approve", approve_url(rental), method: "patch" %>
      </td>
      <td>
        <%= button_to "Deny", deny_url(rental),  method: "patch" %> 
      </td>
    </tr>
    <% end %>
</table>
